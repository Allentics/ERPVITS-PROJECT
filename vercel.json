{
    "headers": [
        {
            "source": "/courses/:slug",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, s-maxage=0, must-revalidate"
                }
            ]
        },
        {
            "source": "/admin/:path*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-store, no-cache, must-revalidate"
                }
            ]
        }
    ]
}